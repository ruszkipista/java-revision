{
  "resourceType": "ValueSet",
  "id": "encounter-status",
  "url": "http://sap.com/healthcare/IS-H/fhir/ValueSet/encounter-status",
  "version": "1.0",
  "name": "EncounterStatus",
  "status": "active",
  "experimental": false,
  "date": "2021-08-01",
  "publisher": "SAP SE",
  "description": "This valueSet defines the possible values for an encounter status. It takes into consideration the values defined for FHIR R5 and the ones that can be supported by IS-H",
  "compose": {
    "include": [
      {
        "system": "http://hl7.org/fhir/encounter-status",
        "concept": [
          {
            "code": "planned",
            "display": "Planned"
          },
          {
            "code": "in-progress",
            "display": "In Progress"
          },
          {
            "code": "finished",
            "display": "Finished"
          },
          {
            "code": "cancelled",
            "display": "Cancelled"
          },
          {
            "code": "entered-in-error",
            "display": "Entered in Error"
          },
          {
            "code": "unknown",
            "display": "Unknown"
          }
        ]
      }
    ]
  }
}